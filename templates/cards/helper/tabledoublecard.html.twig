<div class="row">
{%  for hand in hands %}
        <div class="card text-center col-sm-2 point5 d-flex align-content-center flex-wrap" >

                <div class="card-body">
                    <div class="row">
                        {% for card in hand.cards %}
                            <img src="{{ asset(card| cardImage) }}"  class="doublecard" alt="{{ card| cardImage }}">
                        {% endfor %}
                    </div>
                    <div class="row">
                        <span class="card-title">{{ hand.ratio }}({{hand.win }}/{{ hand.count }})</span>
                    </div>
                </div>
            </div>

   {# {% if not loop.last and (loop.index%10 == 0) %}
    </div><div class="row">
    {% endif %}#}
{% endfor %}
</div>
