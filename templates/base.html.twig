<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="icon" type="image/png" href="{{ asset('/img/logops.png') }}">


        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            <link href="/css/style.css" rel="stylesheet">
            <link href="/css/style-responsive.css" rel="stylesheet">
            <link rel="stylesheet" type="text/css" href="/css/zabuto_calendar.css">
            <link href="https://unpkg.com/bootstrap-table@1.21.0/dist/bootstrap-table.min.css" rel="stylesheet">
            {#<link rel="stylesheet" type="text/css" href="/lib/gritter/css/jquery.gritter.css" />#}

            {#<script src="/lib/chart-master/Chart.js"></script>#}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {#<script src="/lib/chart-master/Chart.js"></script>#}


            {{ encore_entry_script_tags('app') }}

        {% endblock %}
    </head>
    <body>
    <div class="sidebar">
        <ul class="sidebar-menu" id="">
            <p class="centered"><img src="/img/logopokerstar.png" class="img-circle" width="80"></p>
            <li class=" {% if menu_active == 'dashboard' %} active {% else %}sub-menu {% endif %}">
                <a class="{% if menu_active == 'dashboard' %}active{% endif %}" href="/">
                    <i class="fa fa-dashboard"></i>
                    <span>Dashboard</span>
                </a>
            </li>
            <li class=" {% if menu_active == 'tournoi' %} active {% else %}sub-menu {% endif %}">
                <a href="/tournoi" class="{% if menu_active == 'tournoi' %}active{% endif %}">
                    <i class="fa fa-desktop"></i>
                    <span>Tournois</span>
                </a>
            </li>
            <li class=" {% if menu_active == 'hands' %} active {% else %}sub-menu {% endif %}">
                <a href="/hands" class="{% if menu_active == 'hands' %}active{% endif %}">
                    <i class="fa fa-heart"></i>
                    <span>Cards</span>
                </a>
            </li>
            <li class="{% if menu_active == 'players' %} active{% else %}sub-menu {% endif %}">
                <a href="/players" class="{% if menu_active == 'players' %} active{% endif %}">
                    <i class="fa fa-user"></i>
                    <span>Players</span>
                </a>
            </li>
            <li class="{% if menu_active == 'seeds' %} active{% else %}sub-menu {% endif %}" >
                <a href="/seeds" class="{% if menu_active == 'seeds' %} active{% endif %}">
                    <i class="fa fa-seedling"></i>
                    <span>Seeds</span>
                </a>
            </li>
            <li class="{% if menu_active == 'plants' %} active{% else %}sub-menu {% endif %}" >
                <a href="/plant" class="{% if menu_active == 'plants' %} active{% endif %}">
                    <i class="fa fa-plant-wilt"></i>
                    <span>Plantes</span>
                </a>
            </li>
        </ul>
    </div>
    <div class="content">
    {% block body %}{% endblock %}
